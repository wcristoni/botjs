{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "never"
        ],
        "camelcase": [
            2,
            { "properties": "never" }
        ],
        "consistent-return": [
            0
        ]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint"
}
